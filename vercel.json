{
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "risetop/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "risetop/build" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.py" },
    { "src": "/(.*)", "dest": "/risetop/build/$1" }
  ]
}
