{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(vogel|biayaterendah|barat_laut|runall|optimize|lp_max|lp_min)",
      "methods": ["POST"],
      "dest": "logic/app.py"
    },
    {
      "src": "/api/.*",
      "dest": "logic/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
